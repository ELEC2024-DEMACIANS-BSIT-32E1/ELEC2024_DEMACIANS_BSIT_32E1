@{
    ViewData["Title"] = "Personality Page";
}

<body class="PersonalityPage_Body">
    <section class="Personality_Page" id="Personality_Page">
        <div class="Personal_Text_Container">
            <h1>Personality Types</h1>
            <div class="button-container">
                <button class="btn_takequiz">Take a quiz</button>
            </div>
        </div>

        <div class="Analysts-Text-Container">
            <img src="~/images/Analyst_Text_new.png" alt="">
        </div>

        <div class="Personality_Type_Image_Container">
           <div class="vector_container">
                <img class="vector1" src="~/images/Architect.png" alt="">
                <img class="vector2" src="~/images/Logician.png" alt="">
                <img class="vector3" src="~/images/Commander.png" alt="">
                <img class="vector4" src="~/images/Debater.png" alt="">
           </div>
        </div>
    </section>
</body>